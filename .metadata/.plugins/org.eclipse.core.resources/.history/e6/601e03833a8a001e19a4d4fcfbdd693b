package br.com.healthsolu.model;

public class Dieta {
	
	private int id_dieta_indicada;
	private Usuario usuario;
	private float qtd_carboidratos, qtd_proteinas, qtd_calorias;
	
	
	public Dieta() {}
	
	
	public Dieta(int id_dieta_indicada, float qtd_carboidratos, float qtd_proteinas, float qtd_calorias) {
		super();
		this.id_dieta_indicada = id_dieta_indicada;
		this.qtd_carboidratos = qtd_carboidratos;
		this.qtd_proteinas = qtd_proteinas;
		this.qtd_calorias = qtd_calorias;
	}
	public int getId_dieta_indicada() {
		return id_dieta_indicada;
	}
	public void setId_dieta_indicada(int id_dieta_indicada) {
		this.id_dieta_indicada = id_dieta_indicada;
	}
	public float getQtd_carboidratos() {
		return qtd_carboidratos;
	}
	public void setQtd_carboidratos(float qtd_carboidratos) {
		this.qtd_carboidratos = qtd_carboidratos;
	}
	public float getQtd_proteinas() {
		return qtd_proteinas;
	}
	public void setQtd_proteinas(float qtd_proteinas) {
		this.qtd_proteinas = qtd_proteinas;
	}
	public float getQtd_calorias() {
		return qtd_calorias;
	}
	public void setQtd_calorias(float qtd_calorias) {
		this.qtd_calorias = qtd_calorias;
	}
}
